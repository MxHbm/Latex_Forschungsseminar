
@article{bortfeldt_constraints_2013,
  abstract     = {Container loading is a pivotal function for operating supply chains efficiently. Underperformance results in unnecessary costs (e.g. cost of additional containers to be shipped) and in an unsatisfactory customer service (e.g. violation of deadlines agreed to or set by clients). Thus, it is not surprising that container loading problems have been dealt with frequently in the operations research literature. It has been claimed though that the proposed approaches are of limited practical value since they do not pay enough attention to constraints encountered in practice. In this paper, a review of the state-of-the-art in the field of container loading will be given. We will identify factors which ‚Äì from a practical point of view ‚Äì need to be considered when dealing with container loading problems and we will analyze whether and how these factors are represented in methods for the solution of such problems. Modeling approaches, as well as exact and heuristic algorithms will be reviewed. This will allow for assessing the practical relevance of the research which has been carried out in the field. We will also mention several issues which have not been dealt with satisfactorily so far and give an outlook on future research opportunities.},
  author       = {Bortfeldt, Andreas and W√§scher, Gerhard},
  date         = {2013-08},
  doi          = {10.1016/j.ejor.2012.12.006},
  file         = {PDF:/Users/mxhbmn/Zotero/storage/LLEBHTX2/Bortfeldt und W√§scher - 2013 - Constraints in container loading ‚Äì A state-of-the-art review.pdf:application/pdf},
  issn         = {03772217},
  journaltitle = {European Journal of Operational Research},
  langid       = {english},
  number       = {1},
  pages        = {1--20},
  title        = {Constraints in container loading ‚Äì A state-of-the-art review},
  url          = {https://linkinghub.elsevier.com/retrieve/pii/S037722171200937X},
  urldate      = {2025-03-08},
  volume       = {229}
}

