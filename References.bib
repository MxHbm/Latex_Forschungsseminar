  journaltitle = {Operations Research},
  langid       = {english},
  number       = {2},
  pages        = {254--265},
  title        = {Algorithms for the Vehicle Routing and Scheduling Problems with Time Window Constraints},
  url          = {https://pubsonline.informs.org/doi/10.1287/opre.35.2.254},
  urldate      = {2025-03-08},
